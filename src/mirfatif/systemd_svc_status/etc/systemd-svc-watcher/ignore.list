apt-daily.service
apt-daily-upgrade.service
bluetooth.target
dpkg-db-backup.service
fwupd-refresh.service
grub-common.service
grub-initrd-fallback.service
logrotate.service
man-db.service
motd-news.service
plocate-updatedb.service
sleep.target
suspend.target
